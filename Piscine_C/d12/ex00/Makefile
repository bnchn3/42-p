NAME = ft_display_file

all: $(NAME)

$(NAME):
	gcc -Wall -Wextra -Werror ft_display_file.c -o $(NAME)

clean:
	/bin/rm -f *.o

fclean: clean
	/bin/rm -f $(NAME)

re: fclean all
